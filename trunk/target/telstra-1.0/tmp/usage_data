        <td><img src="/res/images/sin.gif" width="58" height="1" border="0" alt="" /></td>
        <td valign="top">
<script type="text/javascript">
<!--
    var scalePixels = 210;
    var scaleMegabytes = 600;
    var scaleImageBase = '';

    var width = Math.round(scalePixels / 30);
    var increment = Math.round(scaleMegabytes / 10);

    var digits = (scaleMegabytes+'').length;
    var infrequent = (digits*7) > (width*2);
    var veryInfrequent = ((digits*7) > (width*3)) && (scaleMegabytes >= 10000);

    var sin = '<img src="/res/images/sin.gif" width="1" height="1" alt="" />';
    var dotTick = '<img src="' + scaleImageBase + '/res/images/scale_tick.gif" width="1" height="1" alt="" />';
    var tinyTick = '<img src="' + scaleImageBase + '/res/images/scale_tick.gif" width="1" height="3" alt="" />';
    var smallTick = '<img src="' + scaleImageBase + '/res/images/scale_tick.gif" width="1" height="6" alt="" />';
    var largeTick = '<img src="' + scaleImageBase + '/res/images/scale_tick.gif" width="1" height="9" alt="" />';

    function spacer(pixels)
    {
        return '<img src="/res/images/sin.gif" width="' + pixels + '" height="1" alt="" />';
    }

    function leftAlign(html, pixels)
    {
        return html + spacer(pixels - 1);
    }

    function rightAlign(html, pixels)
    {
        return spacer(pixels - 1) + html;
    }

    function centerAlign(html, pixels)
    {
        pixels = (pixels - 1) / 2;
        return spacer(pixels) + html + spacer(pixels);
    }

    document.write(spacer(width*36) + '<br />');
    document.write('<table border="0" cellpadding="0" cellspacing="0" width="100%">');

    document.write('<tr>');
    document.write('<td colspan="2">' + sin + '</td>');
    document.write('<td align="right">' + dotTick + '</td>');
    document.write('<td bgcolor="#000000" colspan="30">' + sin + '</td>');
    document.write('<td align="left">' + dotTick + '</td>');
    document.write('<td colspan="2">' + sin + '</td>');
    document.write('</tr>');

    document.write('<tr>');
    document.write('<td>' + leftAlign("", width+1) + '</td>');
    document.write('<td>' + leftAlign("", width+1) + '</td>');
    document.write('<td>' + rightAlign(largeTick, width) + '</td>');

    if (infrequent || veryInfrequent)
    {
        for (var i=0; i<5; i++)
        {
            document.write('<td>' + leftAlign(largeTick, width) + '</td>');
            document.write('<td valign="top">' + centerAlign(tinyTick, width) + '</td>');
            document.write('<td valign="top">' + rightAlign(smallTick, width) + '</td>');
            document.write('<td valign="top">' + leftAlign(smallTick, width) + '</td>');
            document.write('<td valign="top">' + centerAlign(tinyTick, width) + '</td>');
            document.write('<td>' + rightAlign(largeTick, width) + '</td>');
        }
    }
    else
    {
        for (var i=0; i<10; i++)
        {
            document.write('<td>' + leftAlign(largeTick, width) + '</td>');
            document.write('<td valign="top">' + centerAlign(smallTick, width) + '</td>');
            document.write('<td>' + rightAlign(largeTick, width) + '</td>');
        }
    }

    document.write('<td>' + leftAlign(largeTick, width) + '</td>');
    document.write('<td>' + leftAlign("", width+1) + '</td>');
    document.write('<td>' + leftAlign("", width+1) + '</td>');
    document.write('</tr>');

    document.write('<tr>');

    if (veryInfrequent)
    {
        for (var i=0; i<11; i+=2)
        {
            document.write('<td colspan="6" align="center"><small><strong>' + (i*increment/1000) +'</strong></small></td>');
        }
    }
    else if (infrequent)
    {
        for (var i=0; i<11; i+=2)
        {
            document.write('<td colspan="6" align="center"><small><strong>' + (i*increment) +'</strong></small></td>');
        }
    }
    else
    {
        document.write('<td colspan="2">' + sin + '</td>');
        for (var i=0; i<11; i++)
        {
            document.write('<td colspan="2" align="center"><small><strong>' + (i*increment) +'</strong></small></td>');
            document.write('<td colspan="1">' + sin + '</td>');
        }
        document.write('<td colspan="2">' + sin + '</td>');
    }

    document.write('</tr>');

    if (veryInfrequent)
    {
        document.write('<tr>');
        document.write('<td colspan="36" align="center"><small><strong>Gigabytes</strong></small></td>');
        document.write('</tr>');
    }
    else
    {
        document.write('<tr>');
        document.write('<td colspan="36" align="center"><small><strong>Megabytes</strong></small></td>');
        document.write('</tr>');
    }
    document.write('</table>');
//-->
</script>
